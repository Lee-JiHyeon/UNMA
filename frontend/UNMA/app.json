{
  "expo": {
    "name": "UNMA",
    "slug": "UNMA",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.4",
    "orientation": "portrait",

    "icon": "./assets/icon.png",
     
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.ssafy.unma",
      "buildNumber": "1.0.0",
      "requireFullScreen": true,
      "supportsTablet": true
    },
    "android": {
      "package": "com.ssafy.unma",
      "versionCode": 1,
      "permissions": [
          "CAMERA",
          "READ_EXTERNAL_STORAGE",
          "VIBRATE",
          "RECORD_AUDIO"
      ]
    }
  }
}
